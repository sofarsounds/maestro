{
  "name": "@sofarsounds/maestro",
  "version": "0.1.17",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack",
    "prepublishOnly": "yarn run build",
    "moveFiles": "node moveFiles.js",
    "test": "jest --colors --runInBand",
    "test:update": "yarn jest -u"
  },
  "jest": {
    "testRegex": "/.*\\.test\\.(tsx|ts)$",
    "transform": {
      "^.+\\.(tsx|ts)?$": "ts-jest",
      "^.+\\.(ttf|woff)?$": "<rootDir>/src/test/fileMock.ts"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "svg",
      "ttf",
      "woff"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/test/setupTests.ts"
    ],
    "moduleNameMapper": {
      "\\.(eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test/fileMock.ts"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16",
    "react-dom": "^16",
    "typescript": "^3"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "awesome-typescript-loader": "^5.2.1",
    "color": "^3.1.2",
    "enzyme": "^3.10.0",
    "styled-components": "^4",
    "svg-inline-loader": "^0.8.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^4.3.0",
    "@types/color": "^3.0.0",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.14",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "^4.1.16",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-react": "^6.24.1",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest-styled-components": "^6.3.1",
    "terser": "^4.0.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5"
  }
}
