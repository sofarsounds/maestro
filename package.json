{
  "name": "@sofarsounds/maestro",
  "version": "7.0.0",
  "description": "The official sofar sounds react uikit library",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack",
    "postbuild": "rm -rf dist/node_modules",
    "lint": "eslint './src/**/*.{ts,tsx, js, jsx}'",
    "type-check": "tsc --noEmit",
    "test": "jest --colors --runInBand",
    "test:update": "yarn jest -u",
    "start": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "keywords": [],
  "author": "",
  "private": false,
  "license": "ISC",
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16",
    "react-router-dom": "^4",
    "styled-components": "^4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@storybook/addon-a11y": "^5.1.9",
    "@storybook/addon-actions": "^5.1.3",
    "@storybook/addon-knobs": "^5.1.3",
    "@storybook/addon-links": "^5.1.3",
    "@storybook/addon-viewport": "^5.1.3",
    "@storybook/addons": "^5.1.3",
    "@storybook/react": "^5.1.3",
    "@types/color": "^3.0.0",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.14",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "@types/storybook__react": "^4.0.1",
    "@types/styled-components": "^4",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-react": "^6.24.1",
    "color": "^3.1.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "fs-extra": "^8.0.1",
    "gh-pages": "^2.1.1",
    "glob": "^7.1.4",
    "husky": "^3.0.1",
    "image-webpack-loader": "^5.0.0",
    "jest-styled-components": "^6.3.1",
    "react": "^16",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.5",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "enzyme": "^3.10.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "react-dom": "^16",
    "react-router-dom": "^4",
    "styled-components": "^4.3.0",
    "typescript": "^3.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn type-check"
    }
  },
  "jest": {
    "testRegex": "/.*\\.test\\.(tsx|ts)$",
    "transform": {
      "^.+\\.(tsx|ts)?$": "ts-jest",
      "^.+\\.(ttf|woff)?$": "<rootDir>/src/test/fileMock.ts"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "svg",
      "ttf",
      "woff"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/test/setupTests.ts"
    ],
    "moduleNameMapper": {
      "\\.(eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test/fileMock.ts"
    }
  }
}
